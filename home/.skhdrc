ctrl + cmd - f1: startstop --stop -p ~/.mr3_pid.json
ctrl + cmd - f2: startstop --start -p ~/.mr3_pid.json mpv https://mr-stream.connectmedia.hu/4741/mr3.mp3
ctrl + cmd - f3: startstop --start -p ~/.mr3_pid.json mpv https://mr-stream.connectmedia.hu/4691/mr9.mp3

cmd + ctrl - j: yabai -m display --focus west
cmd + ctrl - k: yabai -m display --focus east
shift + cmd - j: yabai -m window --display west --focus
shift + cmd - k: yabai -m window --display east --focus
ctrl + cmd - tab: yabai-focus-change 1
ctrl + cmd + shift - tab: yabai-focus-change -1
ctrl + cmd - m: yabai -m window --grid 1:1:0:0:1:1
